<link rel="stylesheet" href="stylesheets/style.css">

<div>aluno-cadastro</div>

<form action="/private/aluno-cadastro" method="POST">
    <label for="input-username">Nome</label>
    <input type="text" id="input-username" name="nome" placeholder="Nome do aluno" />

    <label for="input-email">Email</label>
    <input type="email" id="input-email" name="email" placeholder="Email" />

    <label for="planos">Planos</label>
    <input type="text" name="planos" id="planos" placeholder="Planos" />

    <label for="inicioPlano">Início do Plano</label>
    <input type="date" id="inicioPlano" name="inicioPlano" placeholder="Data de Inicio" />

    <label for="aniversario">Aniversário</label>
    <input type="date" id="aniversario" name="aniversario"/>

    <label for="telefone">Telefone</label>
    <input type="tel" name="telefone" id="telefone"/>

    <button type="submit">Cadastrar</button>
    
</form>